{% extends 'base.html.twig' %}

{% block title %}Adresse{% endblock %}

{% block body %}
    <h1>Adresse</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adresse.id }}</td>
            </tr>
            <tr>
                <th>Voie</th>
                <td>{{ adresse.voie }}</td>
            </tr>
            <tr>
                <th>Complement</th>
                <td>{{ adresse.complement }}</td>
            </tr>
            <tr>
                <th>BoitePostale</th>
                <td>{{ adresse.boitePostale }}</td>
            </tr>
            <tr>
                <th>CodePostal</th>
                <td>{{ adresse.codePostal }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ adresse.ville }}</td>
            </tr>
            <tr>
                <th>Region</th>
                <td>{{ adresse.region }}</td>
            </tr>
            <tr>
                <th>Departement</th>
                <td>{{ adresse.departement }}</td>
            </tr>
            <tr>
                <th>Pays</th>
                <td>{{ adresse.pays }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ adresse.createdAt ? adresse.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpadatedAt</th>
                <td>{{ adresse.upadatedAt ? adresse.upadatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ContentChanged</th>
                <td>{{ adresse.contentChanged ? adresse.contentChanged|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('adresse_index') }}">back to list</a>

    <a href="{{ path('adresse_edit', {'id': adresse.id}) }}">edit</a>

    {{ include('adresse/_delete_form.html.twig') }}
{% endblock %}
